Host p.giacometti.me
  Hostname p.giacometti.me
  User janrito

Host home
  HostName home.giacometti.me
  User alejandro

Host alejandrogiacometti.com
  Hostname alejandrogiacometti.com
  User janrito

Host *.amazonaws.com aws
  # set default user, override if necessary
  User ubuntu

  # don't update known hosts
  UserKnownHostsFile /dev/null

  # don't check for known hosts
  StrictHostKeyChecking no

  # SOCKS Tunnel
  DynamicForward localhost:54321

Host bitbucket.org
  ControlMaster no

host *.edtd.net
  ForwardAgent yes
  StrictHostKeyChecking no

host 127.0.0.1
  ForwardAgent yes

Host *
  ControlMaster auto
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlPersist 4h
  TCPKeepAlive no
  ServerAliveInterval 60
  ServerAliveCountMax 10

  Protocol 2,1
  Compression no

  # agent
  UseKeychain yes
  AddKeysToAgent yes

  IdentityFile ~/.ssh/id_rsa

  # SOCKS Tunnel
  DynamicForward localhost:65000

  # RMATE
  RemoteForward 52698 localhost:52698

  # IRC
  LocalForward 6030 irc.freenode.net:6667

  # Samba
  LocalForward localhost:9139 app2:139

  # logging
  # LogLevel DEBUG3
  LogLevel QUIET
